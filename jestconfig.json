{
  "transform": {
    "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
  },
  "testRegex": "((/__tests__/.*)(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json"
  ],
	"testEnvironment": "node",
  "cacheDirectory": "./node_modules/.cache/jest",
  "collectCoverageFrom": [
    "src/lib/**/*.{ts,tsx}"
  ]
 }
